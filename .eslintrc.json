{
    "env": {
        "browser": true,
        "es2021": true,
        "node":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"   
    },
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "import/export": 2,
        "no-unused-vars":"off",
        "no-unused-expressions":"off",
        "no-unused-labels":"off",
        "react/react-in-jsx-scope":"off",
        "react/prop-types": ["warn", { "ignore": ["navigation"] }]
    }
}
